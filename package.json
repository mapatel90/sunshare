{
  "type": "module",
  "name": "duralux-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,yellow\" \"next dev --turbo\" \"nodemon server/index.js\"",
    "dev:frontend": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run server:dev\"",
