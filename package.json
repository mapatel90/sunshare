    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "node prisma/seed.js",
    "migration": "node src/scripts/migration-cli.js",
    "migrate": "npm run migration up",
    "migrate:create": "npm run migration create",
    "migrate:status": "npm run migration status",
    "setup:db": "node src/scripts/setup-database.js",
    "test:db": "node src/lib/test-db-connection.js",
    "test:backend": "node test-backend-auth.js",
    "check:roles": "node src/lib/check-roles.js",
    "insert:roles": "node src/lib/insert-roles.js"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.15",
