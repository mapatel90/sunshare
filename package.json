    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "emoji-picker-react": "^4.12.0",
    "express": "^5.1.0",
    "feather-icons": "^4.29.2",
    "feather-icons-react": "^0.7.0",
    "helmet": "^8.1.0",
    "jodit-react": "^4.1.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "next": "14.2.17",
